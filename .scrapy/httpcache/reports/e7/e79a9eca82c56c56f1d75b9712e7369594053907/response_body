        var codeMk = '6007631';
        drawTime();
        clearInterval(timer);
        var timer = setInterval(drawTime, 1000 * 60);
        function drawTime() {
            if (!tiny.browser.ie6 && !tiny.browser.ie7 && !tiny.browser.ie8) {
                var option = {
                    container: "#chartImg",
                    code: codeMk,
                    width: 300,
                    height: 160,
                    onMove: function (msg) {
                        //console.log(msg);
                    }
                }
                var mm = new emcharts3.timemini(option);
                mm.draw();
            } else {
                jQuery("#chartImg").html('<img src="http://pifm.eastmoney.com/EM_Finance2014PictureInterface/Index.aspx?id=' + codeMk + '&imageType=RSDCSP&token=1942f5da9b46b069953c873404aad4b5"/>');
            }
        }
        jQuery("#stat_type").on("click", "li", function () {
            clearInterval(timer);
            if (jQuery(this).hasClass("at2")) return;
            jQuery("#chartImg").empty();
            jQuery(this).addClass("at2").siblings().removeClass("at2");
            if (!tiny.browser.ie6 && !tiny.browser.ie7 && !tiny.browser.ie8) {
                var paramsK = {
                    dpr: 1,
                    code: codeMk,
                    container: "#chartImg",
                    "width": 300,
                    "height": 160
                };
                switch (jQuery(this).attr("id")) {
                    case "cqts_time":
                        drawTime();
                        timer = setInterval(drawTime, 1000 * 60);
                        break;
                    case "cqts_day":
                        paramsK.type = "K";
                        var kmini = new emcharts3.kmini(paramsK);
                        kmini.draw();
                        break;
                    case "cqts_week":
                        paramsK.type = "WK";
                        var kmini = new emcharts3.kmini(paramsK);
                        kmini.draw();
                        break;
                    case "cqts_month":
                        paramsK.type = "MK";
                        var kmini = new emcharts3.kmini(paramsK);
                        kmini.draw();
                        break;

                }
            } else {
                var imageType = jQuery(this).data("img");
                var type = jQuery(this).data("type");
                var imgSrc = 'http://pifm.eastmoney.com/EM_Finance2014PictureInterface/Index.aspx?id=' + codeMk + '&imageType=' + imageType + '&token=1942f5da9b46b069953c873404aad4b5&type='+ type;
                jQuery("#chartImg").html('<img src="' + imgSrc + '"/>');
            }
        })
                var gg = new StockSuggest("hqsearchkey", {
            text: "输代码、名称或简拼",
            type: "ABSTOCK",
            autoSubmit: false,
            width: 220,
            header: ["选项", "代码", "名称", "类型"],
            body: [-1, 1, 4, -2],
            callback: function (ag) {
                window.open('http://quote.eastmoney.com/'+ag.code+'.html');
            }
        });

        function quote_search(bb) {
            var s1 = document.getElementById(bb).value;
            var s = escape(s1);
            if (s1 == "输代码、名称或拼音" || s1 == "" || isNaN(parseInt(s1)) || s1.length != 6) {
                alert("请输入所要查询股票的代码！");
                return false;
            } else {
                var url = 'http://quote.eastmoney.com/' + s1 + '.html';
                window.open(url);
            }
        }
        function zjl_search(bb) {
            var s1 = document.getElementById(bb).value;
            var s = escape(s1);
            if (s1 == "输代码、名称或拼音" || s1 == "" || isNaN(parseInt(s1)) || s1.length != 6) {
                alert("请输入所要查询股票的代码！");
                return false;
            } else {
                var url = '/zjlx/' + s1 + '.html';
                window.open(url);
            }
        }
